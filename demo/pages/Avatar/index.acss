.demo-avatar-size {
  display: flex;
  justify-content: flex-start;
  padding: 24rpx;
}

.demo-avatar-size-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-end;
}
.demo-avatar-size-container + .demo-avatar-size-container {
  margin-left: 16rpx;
}

.demo-avatar-txt {
  padding: 0 16rpx 16rpx;
  font-size: 26rpx;
}

.demo-avatar-content {
  padding: 24rpx;
}
